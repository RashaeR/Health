<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: white;" stylesheets="@registerdoctor.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Health.ReigsterPatientController">
   <children>
      <VBox prefHeight="454.0" prefWidth="500.0" spacing="15.0">
         <children>
            <HBox prefHeight="25.0" prefWidth="222.0" spacing="20.0" stylesheets="@registerdoctor.css">
               <children>
                  <Label text="Name:">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="firstNameText" onKeyTyped="#nameRestriction" prefHeight="25.0" prefWidth="103.0" promptText="First Name">
                     <HBox.margin>
                        <Insets left="12.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="middleText" prefHeight="25.0" prefWidth="29.0" promptText="MI">
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="lastNameText" onKeyTyped="#nameRestriction" prefHeight="25.0" prefWidth="116.0" promptText="Last Name">
                     <HBox.margin>
                        <Insets left="8.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets left="28.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="25.0" prefWidth="198.0" spacing="10.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="64.0" text="Birthday:">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="mDate" onKeyTyped="#dateRestriction" prefHeight="25.0" prefWidth="38.0" promptText="MM">
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="dDate" layoutX="98.0" layoutY="10.0" onKeyTyped="#date2Restriction" prefHeight="25.0" prefWidth="38.0" promptText="DD" />
                  <TextField fx:id="yDate" layoutX="156.0" layoutY="10.0" onKeyTyped="#yearRestriction" prefHeight="25.0" prefWidth="56.0" promptText="YYYY" />
               </children>
               <VBox.margin>
                  <Insets left="28.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="25.0" prefWidth="205.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="78.0" text="Address:">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="addressText" prefHeight="25.0" prefWidth="252.0" promptText="Address ">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets left="28.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="25.0" prefWidth="117.0" spacing="15.0">
               <children>
                  <TextField fx:id="cityText" prefHeight="25.0" prefWidth="141.0" promptText="City">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="stateText" prefHeight="25.0" prefWidth="89.0" promptText="State">
                     <HBox.margin>
                        <Insets left="8.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets left="116.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="25.0" prefWidth="178.0" spacing="28.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="41.0" text="Zip:">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="zipText" onKeyTyped="#zipRestriction" prefHeight="25.0" prefWidth="70.0" promptText="Zip Code">
                     <HBox.margin>
                        <Insets left="9.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets left="28.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="25.0" prefWidth="162.0" spacing="28.0">
               <children>
                  <Label text="Email:">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="emailText" prefHeight="25.0" prefWidth="170.0" promptText="name@domain.com">
                     <HBox.margin>
                        <Insets left="8.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets left="28.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="25.0" prefWidth="184.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="57.0" text="Phone:">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="cellText" onKeyTyped="#phoneRestriction" prefHeight="25.0" prefWidth="96.0" promptText="555-555-5555">
                     <HBox.margin>
                        <Insets left="21.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets left="28.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="25.0" prefWidth="198.0" spacing="5.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="69.0" text="Gender:">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <RadioButton mnemonicParsing="false" text="Male">
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin>
                     <toggleGroup>
                        <ToggleGroup fx:id="genderTG" />
                     </toggleGroup>
                  </RadioButton>
                  <RadioButton mnemonicParsing="false" text="Female" toggleGroup="$genderTG">
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin>
                  </RadioButton>
               </children>
               <VBox.margin>
                  <Insets left="28.0" />
               </VBox.margin>
            </HBox>
         </children>
         <padding>
            <Insets left="40.0" top="50.0" />
         </padding>
      </VBox>
      <Button fx:id="backButton" layoutX="164.0" layoutY="454.0" mnemonicParsing="false" onAction="#showMainMenu" text="Back" />
      <Button layoutX="246.0" layoutY="454.0" mnemonicParsing="false" onAction="#submitInfo" text="Submit" />
   </children>
</AnchorPane>
